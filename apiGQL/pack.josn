{
  "name": "apiLibreriaGQLMongoDB",
  "version": "1.0.0",
  "description": "Sistema de Gestion de una Libreria usando GraphQL y MongoDB",
  "main": "build/app.js",
  "scripts": {
    "start": "node build/app.js",
    "build": "npx tsc -p . && ncp src/schema build/schema",
    "dev": "nodemon \"./src/app.ts\" --exec \"ts-node\" \"./src/app.ts\" -e ts,graphql,json,js",
    "lint": "npx eslint . --ext .ts",
    "lint.fix": "npx eslint . --ext .ts --fix"
  },
  "keywords": [
    "graphql",
    "union",
    "interfaces",
    "query",
    "mutation",
    "mongodb",
    "apollo-server-express-3",
    "jwt"
  ],
  "author": "Jose Antonio Morgado <morgado@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/load-files": "^6.4.0",
    "apollo-server-express": "^3.5.0",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.7.2",
    "graphql-import-node": "^0.0.4",
    "graphql-tools": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.2.1",
    "ncp": "^2.0.0",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@graphql-tools/load-files": "^6.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.7",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0"
  }
}